{
  "name": "frescomer",
  "description": "Opciones para comer en Tijuana",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "data": "node etl/data.js",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "google:clean": "gsutil -m rm -r gs://food.mintitmedia.com/*",
    "google:upload": "gsutil -m cp -r ./__sapper__/export/* gs://food.mintitmedia.com",
    "deploy": "npm run google:clean && npm run google:upload"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "convict": "^6.0.0",
    "debug": "^4.3.1",
    "mapbox-gl": "^1.11.1",
    "mongoose": "^5.11.16",
    "node-fetch": "^2.6.1",
    "polka": "next",
    "sirv": "^0.4.6",
    "stem-porter": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@smui/button": "^1.0.0",
    "@smui/card": "^1.0.0",
    "@smui/dialog": "^1.0.0",
    "@smui/tab": "^1.0.0",
    "@smui/tab-bar": "^1.0.0",
    "mint-components": "^1.0.18",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.24.1"
  }
}
