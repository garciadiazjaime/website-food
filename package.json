{
  "name": "frescomer",
  "description": "Opciones para comer en Tijuana",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "NODE_ENV=production  sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "google:clean": "gsutil -m rm -r gs://food.mintitmedia.com/*",
    "google:upload": "gsutil -m cp -r ./__sapper__/export/* gs://food.mintitmedia.com",
    "deploy": "npm run google:clean && npm run google:upload"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "mapbox-gl": "^1.10.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@smui/button": "^1.0.0-beta.20",
    "@smui/card": "^1.0.0-beta.20",
    "@smui/tab": "^1.0.0-beta.20",
    "@smui/tab-bar": "^1.0.0-beta.20",
    "@smui/dialog": "^1.0.0-beta.20",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0"
  }
}
